<component-set>
	<components>
		<component>
			<role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
			<role-hint>projlib</role-hint>
			<implementation>org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
			</implementation>
			<configuration>
				<phases>
					<generate-sources>
						com.glomidco.maven.plugins:tibco-maven-plugin:${project-version}:generate-sources
					</generate-sources>
					<process-sources>
						com.glomidco.maven.plugins:tibco-maven-plugin:${project-version}:process-sources
					</process-sources>
					<package>
						com.glomidco.maven.plugins:tibco-maven-plugin:${project-version}:package-projlib
					</package>
					<install>
						org.apache.maven.plugins:maven-install-plugin:2.5.1:install
					</install>
					<deploy>
						org.apache.maven.plugins:maven-deploy-plugin:2.8.1:deploy
					</deploy>
				</phases>
			</configuration>
		</component>
		<component>
			<role>org.apache.maven.artifact.handler.DefaultArtifactHandler</role>
			<role-hint>projlib</role-hint>
			<implementation>org.apache.maven.artifact.handler.DefaultArtifactHandler
			</implementation>
			<configuration>
				<type>projlib</type>
				<includeDependencies>true</includeDependencies>
				<language>tibco</language>
				<extension>projlib</extension>
				<addedToClasspath>true</addedToClasspath>
			</configuration>
		</component>
		<component>
			<role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
			<role-hint>ear</role-hint>
			<implementation>org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
			</implementation>
			<configuration>
				<phases>
					<generate-sources>
						com.glomidco.maven.plugins:tibco-maven-plugin:${project-version}:generate-sources
					</generate-sources>
					<process-sources>
						com.glomidco.maven.plugins:tibco-maven-plugin:${project-version}:process-sources
					</process-sources>
					<package>
						com.glomidco.maven.plugins:tibco-maven-plugin:${project-version}:package-ear
					</package>
				</phases>
			</configuration>
		</component>
		<component>
			<role>org.apache.maven.artifact.handler.DefaultArtifactHandler</role>
			<role-hint>ear</role-hint>
			<implementation>org.apache.maven.artifact.handler.DefaultArtifactHandler
			</implementation>
			<configuration>
				<type>ear</type>
				<includeDependencies>true</includeDependencies>
				<language>tibco</language>
				<extension>ear</extension>
				<addedToClasspath>true</addedToClasspath>
			</configuration>
		</component>
	</components>
</component-set>